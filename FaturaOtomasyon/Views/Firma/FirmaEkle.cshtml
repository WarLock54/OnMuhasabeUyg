
@{
    ViewBag.Title = "FirmaEkle";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>FirmaEkle</h2>


 
             
<div class="app-content content ">
    <div class="content-overlay"></div>
    <div class="header-navbar-shadow"></div>
    <div class="content-wrapper container-xxl p-0">
        <div class="content-header row">
            <h4 class="card-title">Firma  Ekle</h4>
        </div>
        <div class="content-body">
            <section class="invoice-list-wrapper">
                <div class="card">
                    <form action="~/Firma/FirmaEkle" method="post" class="form form-vertical">
                  
                        <fieldset>
                            <div class="row">
                                <div class="col-md-12">
                                    <div class="form-group">

                                        <label>
                                            Firma Unvan :
                                        </label>
                                        <input type="text" class="form-control" name="FirmaUnvan" />

                                    </div>
                                </div>
                                <div class="col-md-12">
                                    <div class="form-group">

                                        <label>
                                           AdSoyad :
                                        </label>
                                        <input type="text" class="form-control" name="AdSoyad" />

                                    </div>
                                </div>
                                <div class="col-md-12">
                                    <div class="form-group">
                                        <label>
                                            Email :

                                        </label>
                                        <input type="email" class="form-control" name="Email" />

                                    </div>
                                </div>
                                <div class="col-md-12">
                                    <div class="form-group">
                                        <label>
                                            Telefon :

                                        </label>
                                        <input type="tel" class="form-control" name="Telefon" />

                                    </div>
                                </div>
                                <div class="col-md-12">
                                    <div class="form-group">
                                        <label>
                                            Adres :

                                        </label>
                                        <input type="text" class="form-control" name="Adres" />

                                    </div>
                                </div>
                                <div class="col-md-12">
                                    <div class="form-group">
                                        <label>
                                            VergiAdres :

                                        </label>
                                        <input type="text" class="form-control" name="VergiAdres" />

                                    </div>
                                </div>
                                <div class="col-md-12">
                                    <div class="form-group">
                                        <label>
                                            VergiNo :

                                        </label>
                                        <input type="text" class="form-control" name="VergiNo" />

                                    </div>
                                </div>

                            </div>
                            <button type="submit" class="btn btn-primary">Ekle</button>
                        </fieldset>
                    </form>
                </div>
            </section>
        </div>
    </div>
</div>
